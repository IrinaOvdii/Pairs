<div class="row">
  <div class="col-md-3">
    <ul class="sidebar-list">
    </ul>
  </div>
  <div class="col-md-9">
    <div class="panel panel-primary">
      <div class="panel-heading">
        Your users
      </div>
      <div class="panel-body">

        <% @users.each do |user| %>
        <div class="row">
          <div class="col-md-7">
            <h4><%= user.name %></h4>
          </div>
          <div class="col-md-3 right">
          </div>
        </div>
        <% end %>

      </div>
    </div>

    <%= link_to "Generate new pair", pairs_path, class: "btn btn-primary" %>
  </div>
</div>
