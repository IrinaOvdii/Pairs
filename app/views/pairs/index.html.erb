<div class="row">
  <div class="col-md-3">
    <ul class="sidebar-list">
    </ul>
  </div>
  <div class="col-md-9">
    <div class="panel panel-primary">
      <div class="panel-heading">
        Your pairs
      </div>
      <div class="panel-body">

        <% @pairs.each do |pair| %>
        <div class="row">
          <div class="col-md-7">
            <h4><%= pair.student %> and <%= pair.match %></h4>
          </div>
          <div class="col-md-3 right">
          </div>
        </div>
        <% end %>

      </div>
    </div>

    <%= link_to "Add new pair", new_room_path, class: "btn btn-primary" %>
  </div>
</div>
